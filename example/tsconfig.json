{
	"compilerOptions":
	{
		"target": "es5",
		"module": "commonjs",
		"charset": "utf8",
		//"downlevelIteration": true,
		"importHelpers": true,
		"noEmitHelpers": true,
		"allowJs": false,
		"emitBOM": false,
		"isolatedModules": false,
		"jsx": "react",
		"emitDecoratorMetadata": true,
   		"experimentalDecorators": true,
		"declaration": false,
		"noImplicitAny": false,
		"noImplicitUseStrict": false,
		"removeComments": false,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true,
		"sourceMap": false,
		"moduleResolution": "node",
		"typeRoots": [
		"node_modules/@types"
		],
		"types" : [],
		"lib":
		[
			"es2017",
			"dom",
			"es5",
			"es2015.collection",
			"es2015.iterable"
		]
	},
	"exclude":
	[
		"definitions",
		"vendor"
	],
	"compileOnSave": false,
	"buildOnSave": false,
	"atom":
	{
		"rewriteTsconfig": false
	},
	"fast": "always"
}

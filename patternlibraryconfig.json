{
	"project_info": {
		"name": "DaVanMonet",
		"logo": "./static/logo.png",
		"header_color": "default"
	},
	"directories":
	{
		"src":"./src",
		"cssdest":"/css",
		"build":"./build",
		"onsitepreview":"./onsitepreview",
		"copycsspath":""
	},
	"userconfig": "./user-conf.json",
	"indexing":
	{
		"contentindexoutput":"./contentindex.json",
		"targetindexoutput":"./targetindex.json",
		"keysToOutput":["title","guid","variantid","componentid"]
	},
	"compilation":
	{
		"compilers":{
		"less":
		{
			"taskname":"less",
			"target":".less",
			"compile":true,
			"compileIndividualFiles":false,
			"targets":
			{
				"main.css":
				[
					"**/*.less",
					"!**/*_inline.less",
					"!**/*_print.less",
					"!**/*_exclude.less"
				],
				"inline.css": ["**/*_inline.less", "**/**/*_inline.less"],
				"print.css": ["**/*_print.less", "**/**/*_print.less"],
				"empty.css": ["**/*_asd.less"]
			}
		},
		"scss":{
				"taskname":"sass",
				"target":".scss",
				"compile":false,
				"compileIndividualFiles":false,
				"lint": false
			}
		},
		"sourceMaps":true,
		"copyCompiledCssToFolder":false,
		"minifycss":
		{
			"report": "min",
			"level": 2
		},
		"postcss":
        {
			"map":true,
			"processors": [
				{
					"name":"autoprefixer",
					"options":{ "browsers":["last 2 versions"] }
				}
			]
        }
	},
	"developmentenvironment":
	{
		"devwebsiteport":9001,
		"buildwebsiteport":9002,
		"onsitepreviewport":9003,
		"livereloadport":1234,
		"cssbreakpoints":
		[
			{
				"id": "mobile",
				"title": "Mobile",
				"width": 375,
				"fromWidth": 0,
				"toWidth": 375
			},
			{
				"id": "tablet",
				"title": "Tablet",
				"width": 768,
				"fromWidth": 376,
				"toWidth": 999
			},
			{
				"id": "desktop",
				"title": "Desktop",
				"width": "100%",
				"fromWidth": 1000,
				"toWidth": 9007199254740990
			}
		]
	},
	"structure": [
		{ "title": "Components", "path":"components" },
		{ "title": "Layouts", "path":"layouts" }
	],
	"onsitepreview":
	{
		"components": [ ]
	}
}
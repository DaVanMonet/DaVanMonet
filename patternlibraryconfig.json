			{
				"taskname":"sass",
				"target":".scss",
				"compile":true,
				"compileIndividualFiles":true
			}
		],
		"sourceMaps":true
	},
	"preview":
	{
		"websiteport":9001
	},
	"developmentOptions":
	{
		"livereloadport":1234
	},
	"structure": 
	[
		{ "title": "Examples", "path":"examples" }
	]
}
{	
	"directories":
	{
		"src":"./src",
		"dest":"./build/css"
	},
	"indexingOptions":
	{
		"output":"./contentindex.json",
		"keysToOutput":["title","guid"]
	},
	"compilation":
	{
		"compilers":
		[
			{
				"taskname":"less",
				"target":".less",
				"compile":true,

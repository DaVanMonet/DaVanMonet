{	
	"directories":
	{
		"src":"./src",
		"cssdest":"./build/css",
		"build":"./build"
	},
	"userconfig": "./user-conf.json",
	"indexing":
	{
		"output":"./contentindex.json",
		"keysToOutput":["title","guid","variantid","componentid"]
	},
	"compilation":
	{
		"compilers":
		[
			{
				"taskname":"less",
				"target":".less",
				"compile":true,
				"compileIndividualFiles":false,
				"targets":
				{
					"main.css":
					[
						"**/*.less",
						"!**/*_inline.less",
						"!**/*_print.less",
						"!**/*_exclude.less"
					],
					"inline.css": ["**/*_inline.less", "**/**/*_inline.less"],
					"print.css": ["**/*_print.less", "**/**/*_print.less"],
					"empty.css": ["**/*_asd.less"]
				}
			},
			{
				"taskname":"sass",
				"target":".scss",
				"compile":false,
				"compileIndividualFiles":false
			}
		],
		"minifycss":true,
		"sourceMaps":true,
		"postcss":
        {
			"map":true,
			"processors": [
				{
					"name":"autoprefixer",
					"options":{ "browsers":["last 2 versions"] }
				}
			]
        }
	},
	"developmentenvironment":
	{
		"devwebsiteport":9001,
		"buildwebsiteport":9002,
		"livereloadport":1234
	},
	"structure": 
	[
		{ "title": "Examples", "path":"examples" },
		{ "title": "Components", "path":"components" },
		{ "title": "Layouts title", "path":"layouts" }
	]
}
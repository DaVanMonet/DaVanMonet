{	
	"directories":
	{
		"src":"./src",
		"cssdest":"./build/css",
		"build":"./build"
	},
	"indexing":
	{
		"output":"./contentindex.json",
		"keysToOutput":["title","guid"]
	},
	"compilation":
	{
		"compilers":
		[
			{
				"taskname":"less",
				"target":".less",
				"compile":true,
				"compileIndividualFiles":true
			},
			{
				"taskname":"sass",
				"target":".scss",
				"compile":true,
				"compileIndividualFiles":true
			}
		],
		"sourceMaps":true,
		"postcss":
        {
			"map":true,
			"processors": [
				{
					"name":"autoprefixer",
					"options":{ "browsers":["last 2 versions"] }
				}
			]
        }
	},
	"developmentenvironment":
	{
		"devwebsiteport":9001,
		"buildwebsiteport":9002,
		"livereloadport":1234
	},
	"structure": 
	[
		{ "title": "Examples", "path":"examples" },
		{ "title": "Components", "path":"components" },
		{ "title": "Layouts title", "path":"layouts" }
	]
}